-- Drop table

-- DROP TABLE public.vitalrecords;

CREATE TABLE public.vitalrecords (
	vitalrecordid serial4 NOT NULL,
	userid int4 NOT NULL,
	"date" date NOT NULL,
	vitaltypeid int4 NOT NULL,
	value varchar NOT NULL,
	CONSTRAINT vitalrecords_pk PRIMARY KEY (vitalrecordid)
);
